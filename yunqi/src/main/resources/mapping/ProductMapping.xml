<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yunqi.common.mapper.ProductMapper">
    <resultMap id="BaseResultMap" type="com.yunqi.common.entity.Product">
        <result column="id" jdbcType="INTEGER" property="id"/>
        <result column="name" jdbcType="VARCHAR" property="name"/>
        <result column="code" jdbcType="VARCHAR" property="code"/>
        <result column="classification" jdbcType="VARCHAR" property="classification"/>
        <result column="shipType" jdbcType="VARCHAR" property="shipType"/>
        <result column="price" jdbcType="VARCHAR" property="price"/>
        <result column="disabled" jdbcType="VARCHAR" property="disabled"/>
        <result column="createName" jdbcType="VARCHAR" property="createName"/>
        <result column="createTime" jdbcType="TIMESTAMP" property="createTime"/>
        <result column="updateName" jdbcType="VARCHAR" property="updateName"/>
        <result column="updateTime" jdbcType="TIMESTAMP" property="updateTime"/>
    </resultMap>

    <select id="select" resultType="com.yunqi.common.entity.Product">
        select * from product
    </select>

    <insert id="insert" parameterType="com.yunqi.common.entity.Product" useGeneratedKeys="true" keyProperty="id">
        insert into product (name, code, classification, shipType, price) value (
            #{name}, #{code}, #{classification}, #{shipType}, #{price}
        )
    </insert>

    <insert id="update" parameterType="com.yunqi.common.entity.Product">

    </insert>

    <delete id="delete" parameterType="com.yunqi.common.entity.Product">

    </delete>
</mapper>