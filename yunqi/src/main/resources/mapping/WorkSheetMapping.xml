<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yunqi.common.mapper.WorkSheetMapper">
    <resultMap id="BaseResultMap" type="com.yunqi.common.entity.WorkSheet">
        <result column="id" jdbcType="INTEGER" property="id"/>
        <result column="projectId" jdbcType="INTEGER" property="projectId"/>
        <result column="techIds" jdbcType="VARCHAR" property="techIds"/>
        <result column="dateOfVisit" jdbcType="TIMESTAMP" property="dateOfVisit"/>
        <result column="orderDate" jdbcType="TIMESTAMP" property="orderDate"/>
        <result column="type" jdbcType="VARCHAR" property="type"/>
        <result column="status" jdbcType="VARCHAR" property="status"/>
        <result column="actualWork" jdbcType="VARCHAR" property="actualWork"/>
        <result column="schedule" jdbcType="VARCHAR" property="schedule"/>
        <result column="watermark" jdbcType="VARCHAR" property="watermark"/>
        <result column="signImg" jdbcType="VARCHAR" property="signImg"/>
        <result column="signTime" jdbcType="TIMESTAMP" property="signTime"/>
        <result column="writeTime" jdbcType="TIMESTAMP" property="writeTime"/>
        <result column="disabled" jdbcType="VARCHAR" property="disabled"/>
        <result column="createName" jdbcType="VARCHAR" property="createName"/>
        <result column="createTime" jdbcType="TIMESTAMP" property="createTime"/>
        <result column="updateName" jdbcType="VARCHAR" property="updateName"/>
        <result column="updateTime" jdbcType="TIMESTAMP" property="updateTime"/>
    </resultMap>

    <select id="select" resultType="com.yunqi.common.entity.WorkSheet">
        select *
        from worksheet w
    </select>

    <insert id="insert" parameterType="com.yunqi.common.entity.WorkSheet" useGeneratedKeys="true" keyProperty="id">
        insert into worksheet (projectId, techIds, dateOfVisit, orderDate,
            type, status, actualWork, schedule,
            watermark, signImg, signTime, writeTime
        ) value (
           #{projectId}, #{techIds}, #{dateOfVisit}, #{orderDate},
           #{type}, #{status}, #{actualWork}, #{schedule},
           #{watermark}, #{signImg}, #{signTime}, #{writeTime}
        )
    </insert>

    <insert id="update" parameterType="com.yunqi.common.entity.WorkSheet">

    </insert>

    <insert id="sign" parameterType="com.yunqi.common.entity.WorkSheet">

    </insert>

    <insert id="write" parameterType="com.yunqi.common.entity.WorkSheet">

    </insert>

    <delete id="delete" parameterType="com.yunqi.common.entity.WorkSheet">

    </delete>
</mapper>